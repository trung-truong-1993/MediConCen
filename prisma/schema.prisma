generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
  moduleFormat = "cjs"
}

datasource db {
  provider = "mysql"
}

model UserMapping {
  id      BigInt @id @default(autoincrement())
  id1     String
  id2     String
  userId  String  @db.Char(36)

  @@unique([id1, id2])
  @@map("user_mappings")
}